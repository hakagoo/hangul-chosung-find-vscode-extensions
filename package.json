{
	"name": "qlee3",
	"displayName": "한글 초성 변수 찾기",
	"description": "한글 초성으로 한글 변수, 함수명 찾기",
	"publisher": "qlee3",
	"repository": {
		"type": "git",
		"url": "https://github.com/qlee3/hangul-chosung-find-vscode-extensions.git"
	},
	"license": "MIT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "qlee3.chosung",
				"title": "Hello 초성으로 찾기"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"glob": "^7.1.6",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"mocha": "^8.2.1",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "yarn lint --fix"
	}
}
